{
  "PartnerUserDB": {
    "ConnectionString": "server=localhost;userid=partnership;password=Password@123;database=PartnerUserDB;"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "Elasticsearch",
        "Args": {
          "nodeUris": "http://localhost:9200",
          "indexFormat": "ofx-15.dev-partneruser-{0:yyyy.MM.dd}",
          "templateName": "ofx-*"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },
  "ProjectsDetails": {
    "Title": "PartnerUser Service",
    "DocumentationTitle": "PartnerUser API Documentation",
    "Description": "A simple PartnerUser Solution",
    "ContactName": "Partnership Team"
  },
  "AllowedHosts": "*"
}
